import java.io.*;
class test  
{
	public static void main (String[] args) throws java.lang.Exception
	{
		int[] arr = new int[]{4,6,3,2,-6,9,90,23};
	     for(int i = 0;i < arr.length -1 ; i++) {
	         for(int j = i;j < arr.length -1; j++) {
	             if(arr[i] > arr[j+1]) {
	               int temp = arr[i];
    	           arr[i] = arr[j + 1];
    	           arr[j +1] = temp;
	           }
	         }
	     }
		
		for(int a : arr) {
			System.out.print(a+",");
		}
	}
}
